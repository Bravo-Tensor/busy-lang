{
  "name": "busy-cli",
  "version": "0.1.3",
  "description": "CLI for BUSY document framework - parse, validate, and manage BUSY workspaces",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "busy": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "busy": "node ./dist/cli/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bravo-Tensor/busy-lang.git",
    "directory": "packages/busy-cli"
  },
  "homepage": "https://github.com/Bravo-Tensor/busy-lang#readme",
  "bugs": {
    "url": "https://github.com/Bravo-Tensor/busy-lang/issues"
  },
  "dependencies": {
    "zod": "^3.23.8",
    "gray-matter": "^4.0.3",
    "unified": "^11.0.5",
    "remark-parse": "^11.0.0",
    "remark-frontmatter": "^5.0.0",
    "unist-util-visit": "^5.0.0",
    "github-slugger": "^2.0.0",
    "fast-glob": "^3.3.2",
    "commander": "^12.1.0",
    "debug": "^4.3.7"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@types/debug": "^4.1.12",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  },
  "keywords": [
    "busy",
    "cli",
    "parser",
    "markdown",
    "workspace",
    "agent",
    "prompt-engineering"
  ],
  "author": "Bravo Tensor",
  "license": "ISC"
}
